{
  "version": 2,
  "builds": [
    {
      "src": "ggvibe/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "ggvibe/build" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/ggvibe/static/$1" },
    { "src": "/(.*)", "dest": "/ggvibe/build/index.html" }
  ]
}
